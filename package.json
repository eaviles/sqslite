{
  "name": "sqslite",
  "version": "1.0.0",
  "description": "Lightweight module for integration testing AWS SQS.",
  "keywords": [],
  "author": "Jenny Eckstein <Jenny.Eckstein@ADP.com>",
  "maintainers": [
    "Edgardo Avil√©s <Edgardo.Aviles@ADP.com>",
    "Jenny Eckstein <Jenny.Eckstein@ADP.com>"
  ],
  "contributors": [
    "Chen Doron <Chen.Doron@ADP.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifion/sqslite.git"
  },
  "bugs": {
    "url": "https://github.com/lifion/sqslite/issues"
  },
  "homepage": "https://lifion.github.io/sqslite",
  "main": "lib/index.js",
  "bin": "bin/cli.js",
  "engines": {
    "node": ">=v10.23.3"
  },
  "scripts": {
    "build-docs": "jsdoc2md -t ./templates/README.hbs ./lib/*.js > ./README.md && git add ./README.md",
    "build-docs-watch": "npm-watch build-docs",
    "eslint": "eslint . --ext .js,.json --ignore-pattern='!.*.*'",
    "format": "prettier --write '**/*.{md,js,json,ts}' '!coverage/**/*.{js,json}'",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest -c ./.jest.json",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "fastify": "^3.17.0",
    "fastify-formbody": "^5.0.0",
    "md5": "^2.3.0",
    "minimist": "^1.2.5",
    "superstruct": "^0.15.1",
    "uuid": "^8.3.2",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "auto-changelog": "^2.3.0",
    "aws-sdk": "^2.925.0",
    "chalk": "^4.1.1",
    "check-engines": "^1.5.0",
    "codecov": "^3.8.2",
    "eslint": "^7.28.0",
    "eslint-config-lifion": "^2.1.1",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-xml-matcher": "^1.2.0",
    "jsdoc-to-markdown": "^7.0.1",
    "lint-staged": "^10.5.4",
    "npm-watch": "^0.9.0",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "semver": "^7.3.5",
    "typescript": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-docs && lint-staged",
      "pre-push": "npm run eslint && npm test"
    }
  },
  "watch": {
    "build-docs": {
      "patterns": [
        "lib"
      ]
    }
  },
  "@lifion/core-commons": {
    "template": "public",
    "updated": "2020-06-26T20:08:10.211Z",
    "version": "2.4.0"
  }
}
